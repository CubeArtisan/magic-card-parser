    "lib": "src",
    "example": "scripts",
    "test": "__tests__"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruler501/magic-card-parser.git"
  },
  "homepage": "https://github.com/ruler501/magic-card-parser",
  "bugs": {
